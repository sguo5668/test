@model IEnumerable<DIA.Web.ReferenceTableValue>

@{

    ViewData["Title"] = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SystemValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TableName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SystemValue)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.TableName)
            </td>
        </tr>
    }
</table>

<div>@ViewBag.Status</div>
